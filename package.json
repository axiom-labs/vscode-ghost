{
    "name": "ghost-lang",
    "displayName": "Ghost Lang",
    "icon": "icon.png",
    "description": "Ghost extension for Visual Studio Code.",
    "publisher": "axiom-labs",
    "version": "0.0.2",
    "license": "MIT",
    "keywords": ["ghost"],
    "private": true,
    "bugs": {
        "url": "https://github.com/axiom-labs/vscode-ghost/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/axiom-labs/vscode-ghost.git"
    },
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ghost",
            "aliases": ["Ghost", "ghost"],
            "extensions": [".ghost"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ghost",
            "scopeName": "source.ghost",
            "path": "./syntaxes/ghost.tmLanguage.json"
        }]
    }
}